{
  "name": "node-express-typescript",
  "description": "node-express-typescript Sample Application (TypeScript)",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "set NODE_ENV=production&& node ./out/www.js",
    "start:desk": "set NODE_ENV=desktop&& node ./out/www.js",
    "test-start": "set NODE_ENV=test&& node ./out/www.js",
    "test": "npm run compiletests && set NODE_ENV=test&& mocha ./tests/out/tests/**/test*.js",
    "compile": "node ./node_modules/typescript/bin/tsc -p ./src",
    "compiletests": "node ./node_modules/typescript/bin/tsc -p ./tests",
    "buildandrun": "gulp build && npm start",
    "build": "gulp build",
    "postinstall": "cd frontend/ && npm install",
    "buildProd": "cd frontend/ && npm run build && cd .. && gulp buildAll",
    "buildDesk": "cd frontend/ && npm run buildDesk && cd .. && gulp buildAll"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "~1.13.2",
    "browser-sync": "^2.18.8",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-rx": "0.0.4",
    "cookie-parser": "~1.3.5",
    "cors": "^2.8.4",
    "couchbase": "^2.6.0",
    "debug": "~2.2.0",
    "del": "^3.0.0",
    "express": "~4.13.1",
    "gulp": "^3.9.1",
    "gulp-less": "^4.0.1",
    "gulp-mocha": "^6.0.0",
    "gulp-nodemon": "^2.4.1",
    "gulp-tsb": "^2.0.5",
    "jsonwebtoken": "^8.3.0",
    "lokijs": "^1.5.1",
    "marked": "^0.3.6",
    "md5": "^2.2.1",
    "mocha": "^3.2.0",
    "morgan": "~1.6.1",
    "multer": "^1.3.0",
    "nodemailer": "^4.1.3",
    "nodemon": "^1.11.0",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "request-promise-native": "^1.0.5",
    "rxjs": "^5.4.3",
    "save": "^2.3.1",
    "serve-favicon": "~2.3.0",
    "typescript": "^2.2.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.16.1",
    "@types/chai": "^4.0.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/cookie-parser": "^1.3.30",
    "@types/debug": "^0.0.29",
    "@types/del": "^3.0.0",
    "@types/express": "^4.0.35",
    "@types/gulp": "^4.0.5",
    "@types/gulp-less": "0.0.31",
    "@types/gulp-mocha": "0.0.32",
    "@types/gulp-nodemon": "0.0.32",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/lokijs": "^1.2.30",
    "@types/marked": "^0.0.28",
    "@types/mocha": "^2.2.40",
    "@types/morgan": "^1.7.32",
    "@types/multer": "^1.3.5",
    "@types/node": "^7.0.10",
    "@types/power-assert": "^1.4.29",
    "@types/request": "^2.47.0",
    "@types/request-promise": "^4.1.41",
    "@types/request-promise-native": "^1.0.15",
    "@types/serve-favicon": "^2.2.28"
  }
}
